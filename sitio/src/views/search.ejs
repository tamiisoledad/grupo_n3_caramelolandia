<!DOCTYPE html>
<html lang="es">
    <%- include('partials/head',{title: "Resultado de la busqueda"})%>
<body class="busqueda">
    <%- include('partials/header')%>
    <main>
        <section>
            <div>
                <% productos.forEach(producto => { %>
                    <article>
                         <a class="product" href="/products/detail/<%= producto.id %> "><img src="/images/productos/<%= producto.Imagen%>" alt=""></a>
                         <div>
                         <h4><%= producto.Producto %> </h4>
                        </div>
                         <p>$<%= producto.Precio %> </p>
                         <a class="carrito" href="/products/cart">a√±adir al carrito</a>
                    </article>
                <% }) %>
            </div>
        
        </section>
    </main>
    <%- include('partials/footer')%>
</body>
</html>